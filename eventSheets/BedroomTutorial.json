				{
					"callFunction": "dialogue",
					"sid": 649490528673953,
					"parameters": [
						"\"That’s about it for now, Thank you guys so much for helping!\"",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 358365870033939,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 395604143063786,
					"parameters": [
						"\"Wouldn’t have it any other way! It is getting late so we’ll give you some time to settle in now, don’t hesitate to reach out if you ever need anything.\"",
						"\"Dad\""
					]
				},
				{
					"id": "wait-for-signal",
