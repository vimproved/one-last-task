			"actions": [
				{
					"callFunction": "dialogue",
					"sid": 224285840029679,
					"parameters": [
						"\"Okay! That box goes in that corner, and the chair goes over there.\"",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 359347160785906,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 195105463749934,
					"parameters": [
						"\"Alright, sweetie! What about your desk and computer?\"\n",
						"\"Mom\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 434967376376727,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 384357146558028,
					"parameters": [
						"\"Hmm… We’ll put it against that wall.\"\n",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 437647970925870,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 718443694914890,
					"parameters": [
						"\"This really is a nice apartment! How much did they say you were paying for rent again?\"\n",
						"\"Dad\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 708155096749715,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 270553918806592,
					"parameters": [
						"\"Dad! I told you I have it covered. I have enough saved to pay rent for 6 months, and I’m about to get a job as a spellcrafter, so then I won’t ever have to worry about it again.\"",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 924107025435184,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 453384953402236,
					"parameters": [
						"\"I know, but are you sure you want to do this before you have the job?\"",
						"\"Dad\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 485848612802317,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 363600753908007,
					"parameters": [
						"\"Yes! It’ll be fine, I already have my certifications, all I have to do is apply.\"\n",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 108324703562970,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 206197771265927,
					"parameters": [
						"\"Carper, we’ve already had this talk, and they’ve already signed the lease. It’ll be fine, but if need be we’ll cross that bridge when we come to it.\"",
						"\"Mom\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 859920897244152,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 629175903753231,
					"parameters": [
						"\"You’re right, I’m sorry Opal.\"\n",
						"\"Dad\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
					"sid": 192231599028603,
					"parameters": {
						"tag": "\"Next\""
					}
				},
				{
					"callFunction": "dialogue",
					"sid": 277442542063357,
					"parameters": [
						"\"It’s alright. Trust me, I’ve got this!\"\n",
						"\"Opal\""
					]
				},
				{
					"id": "wait-for-signal",
					"objectClass": "System",
